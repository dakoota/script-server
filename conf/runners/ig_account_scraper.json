{
  "hidden": true,
  "name": "IG Account Scraper",
  "script_path": "python scripts/ig_posts_by_account.py",
  "description": "\nEnter the username (without @) and maximum number of posts to scrape (default is 10,000) and click 'Execute'.",
  "parameters": [
    {
      "name": "Instagram Username",
      "required": true,
      "type": "str",
      "param": "--uname",
      "description": "Instagram Username"
    },
    {
      "name": "Maximum Posts",
      "required": true,
      "type": "int",
      "default": "10000",
      "param": "--max",
      "description": "Maximum Posts"
    }
  ],
  "output_files": [
      "outputs/${Instagram Username}-posts.csv"
  ]
}
